{
  "name": "@kriptiko/vue-table",
  "description": "Vue table component for rendering server-side data.",
  "keywords": [
    "vue-table",
    "pagination",
    "search",
    "sorting",
    "filters",
    "reorderable",
    "bootstrap4-theme",
    "datatables"
  ],
  "version": "1.1.2",
  "license": "MIT",
  "private": false,
  "author": "José Osório",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KriptikoCreativeStudio/vue-table.git"
  },
  "main": "./dist/vue-table.common.js",
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js",
    "*.md"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/app.js",
    "build": "vue-cli-service build dev/app.js",
    "build-bundle": "vue-cli-service build --target lib --name vue-table ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.0",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "bugs": {
    "url": "https://github.com/KriptikoCreativeStudio/vue-table/issues"
  },
  "homepage": "https://github.com/KriptikoCreativeStudio/vue-table#readme"
